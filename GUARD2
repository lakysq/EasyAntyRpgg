
local lp = game.Players.LocalPlayer
local hwid = game:GetService("RbxAnalyticsService"):GetClientId()
local White = true
local WhiteList = {"aboba9821"}
for index = 1, #WhiteList do
    if WhiteList[index] == hwid
        White = false
    end
end
if not White then
    lp:Kick("you are not on the whitelist")
end
