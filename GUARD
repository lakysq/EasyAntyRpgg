loadstring(game:HttpGet("https://raw.githubusercontent.com/lakysq/EasyAntyRpgg/refs/heads/main/LOGI", true ))()
checkfor1send = 0
local lpn = game.Players.LocalPlayer.Name
local lp = game.Players.LocalPlayer
local White = false
local WhiteList = {'Angel_a324','nazarkusalt3','alyerhelper3001','themost_legit','Zigdiger','king_hvh123','nazarkus_hvh1337','trprprprororif'}
for index = 1, #WhiteList do
    if WhiteList[index] == lpn then
        White = true
    end
end
if not White then
    lp:Kick("You don't in WhiteList")
end
